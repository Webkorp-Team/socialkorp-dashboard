{
  "name": "socialkorp-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "handle-upper-level-config": "cd .. && npm run handle-config",
    "handle-config": "npm run handle-upper-level-config && node copy-config.js",
    "dev": "NODE_ENV='development' npm run handle-config && NODE_ENV='development' node server.js",
    "emulators": "cd functions && npm run init && cd .. && firebase emulators:start", 
    "build": "npm run handle-config && next build && npm run export",
    "export": "next export -o build",
    "start": "next start",
    "deploy": "npm run build && firebase deploy --only hosting:admin"
  },
  "dependencies": {
    "@babel/core": "^7.15.0",
    "@babel/plugin-proposal-private-methods": "^7.14.5",
    "express": "^4.17.1",
    "file-loader": "^6.2.0",
    "http-proxy-middleware": "^2.0.1",
    "immutable": "^4.0.0-rc.14",
    "install": "^0.13.0",
    "next": "^11.1.0",
    "npm": "^7.20.6",
    "query-string": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "style-loader": "^3.2.1",
    "styled-components": "^5.3.0",
    "styled-components-responsive-toolkit": "github:Webkorp-Team/styled-components-responsive-toolkit"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.12.0",
    "redux-devtools-extension": "^2.13.9"
  }
}
